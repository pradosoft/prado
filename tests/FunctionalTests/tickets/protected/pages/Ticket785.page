<com:TContent ID="Content">
<com:TDatePicker Id="datePicker" InputMode="DropDownList">
		<prop:ClientSide.OnDateChanged>
			$('#selDate').html(parameter);
		</prop:ClientSide.OnDateChanged>
</com:TDatePicker>
<p>
Client Side Selected date : <span id="selDate"></span>
</p>


